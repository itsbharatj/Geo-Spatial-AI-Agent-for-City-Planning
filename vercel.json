{
    "version": 2,
    "builds": [
        {
            "src": "backend.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend.py"
        },
        {
            "src": "/plan",
            "dest": "backend.py"
        },
        {
            "src": "/health",
            "dest": "backend.py"
        },
        {
            "src": "/",
            "dest": "frontend/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/$1"
        }
    ]
}