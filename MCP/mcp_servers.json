{
    "duckduckgo": {
        "command": "docker",
        "args": [
            "run",
            "-i",
            "--rm",
            "mcp/duckduckgo"
        ],
        "transport": "stdio"
    }, 
    "ACICN-MCP-AQI":{
        "command":"uv", 
        "args":[
            "run", 
            "services/AQI/aqicn-mcp/aqicn_server.py"
        ], 
        "transport":"stdio"
    }, 
    "VISUAL-CROSSING-WEATHER":{
        "command":"uv", 
        "args":[
            "run", 
            "services/weather/visual_crossing_mcp/visual_crossing_mcp.py"
        ], 
        "transport":"stdio"
    }, 
    "TOMTOM-TRAFFIC": {
        "command": "npx",
        "args": ["-y", "@tomtom-org/tomtom-mcp@latest"],
        "transport": "stdio",
        "env": {
            "TOMTOM_API_KEY": "$TOMTOM_API_KEY"
    }
    }, 
    "DATA-COMMONS-GOOGLE": {
        "command": "uvx",
        "args": ["datacommons-mcp","serve", "stdio"],
        "transport": "stdio",
        "env": {
            "DC_API_KEY": "$DC_API_KEY"
    }
    }

}